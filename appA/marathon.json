{
  "id": "/appa",
  "instances": 1,
  "cpus": 1,
  "mem": 128,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "philipnrmn/os101-demo:appA",
      "network": "BRIDGE",
      "portMappings": [
        {
          "hostPort": 0,
          "containerPort": 5000,
          "protocol": "tcp"
        }
      ]
    }
  },
  "env": {
    "HAPROXY_GROUP": "external",
    "HAPROXY_0_VHOST": "alice.os101.mesosphere.com"
  },
  "healthChecks": [
    {
      "portIndex": 0,
      "protocol": "HTTP",
      "path": "/"
    }
  ]
}

